  <xs:complexType name="distributionListGranteeSelector">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="type" type="ns1:granteeType" use="required"/>
        <xs:attribute name="by" type="ns1:distributionListGranteeBy" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="distributionListRightSpec">
    <xs:sequence>
      <xs:element name="grantee" type="tns:distributionListGranteeSelector" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="right" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="distributionListSubscribeReq">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="op" type="tns:distributionListSubscribeOp" use="required"/>
        <xs:attribute name="bccOwners" type="xs:boolean"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="distributionListActionResponse">
    <xs:sequence/>
  </xs:complexType>

