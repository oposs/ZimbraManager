  <xs:complexType name="authRequest">
    <xs:all>
      <xs:element name="account" type="ns1:accountSelector" minOccurs="0"/>
      <xs:element name="password" type="xs:string" minOccurs="0"/>
      <xs:element name="preauth" type="tns:preAuth" minOccurs="0"/>
      <xs:element name="authToken" type="tns:authToken" minOccurs="0"/>
      <xs:element name="virtualHost" type="xs:string" minOccurs="0"/>
      <xs:element name="prefs" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="pref" type="tns:pref" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="attrs" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="attr" type="tns:attr" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="requestedSkin" type="xs:string" minOccurs="0"/>
    </xs:all>
    <xs:attribute name="persistAuthTokenCookie" type="xs:boolean"/>
  </xs:complexType>

  <xs:complexType name="preAuth">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="timestamp" type="xs:long" use="required"/>
        <xs:attribute name="expiresTimestamp" type="xs:long"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="authToken">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="verifyAccount" type="xs:boolean"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="pref">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="modified" type="xs:long"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="attr">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="pd" type="xs:boolean"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="authResponse">
    <xs:all>
      <xs:element name="authToken" type="xs:string"/>
      <xs:element name="lifetime" type="xs:long"/>
      <xs:element name="session" type="tns:session" minOccurs="0"/>
      <xs:element name="refer" type="xs:string" minOccurs="0"/>
      <xs:element name="skin" type="xs:string" minOccurs="0"/>
      <xs:element name="prefs" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="pref" type="tns:pref" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="attrs" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="attr" type="tns:attr" minOccurs="0" maxOccurs="unbounded"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="session">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="type" type="xs:string"/>
        <xs:attribute name="id" type="xs:string" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="autoCompleteGalRequest">
    <xs:sequence/>
    <xs:attribute name="needExp" type="xs:boolean"/>
    <xs:attribute name="name" type="xs:string" use="required"/>
    <xs:attribute name="type" type="ns1:galSearchType"/>
    <xs:attribute name="galAcctId" type="xs:string"/>
    <xs:attribute name="limit" type="xs:int"/>
  </xs:complexType>

  <xs:complexType name="autoCompleteGalResponse">
    <xs:sequence>
      <xs:element name="cn" type="tns:contactInfo" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="more" type="xs:boolean"/>
    <xs:attribute name="tokenizeKey" type="xs:boolean"/>
    <xs:attribute name="paginationSupported" type="xs:boolean"/>
  </xs:complexType>

  <xs:complexType name="contactInfo">
    <xs:sequence>
      <xs:element ref="tns:meta" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="a" type="ns1:contactAttr" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="m" type="tns:contactGroupMember" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="sf" type="xs:string"/>
    <xs:attribute name="exp" type="xs:boolean"/>
    <xs:attribute name="id" type="xs:string" use="required"/>
    <xs:attribute name="l" type="xs:string"/>
    <xs:attribute name="f" type="xs:string"/>
    <xs:attribute name="t" type="xs:string"/>
    <xs:attribute name="tn" type="xs:string"/>
    <xs:attribute name="md" type="xs:long"/>
    <xs:attribute name="ms" type="xs:int"/>
    <xs:attribute name="d" type="xs:long"/>
    <xs:attribute name="rev" type="xs:int"/>
    <xs:attribute name="fileAsStr" type="xs:string"/>
    <xs:attribute name="email" type="xs:string"/>
    <xs:attribute name="email2" type="xs:string"/>
    <xs:attribute name="email3" type="xs:string"/>
    <xs:attribute name="type" type="xs:string"/>
    <xs:attribute name="dlist" type="xs:string"/>
    <xs:attribute name="ref" type="xs:string"/>
    <xs:attribute name="tooManyMembers" type="xs:boolean"/>
    <xs:attribute name="isOwner" type="xs:boolean"/>
    <xs:attribute name="isMember" type="xs:boolean"/>
  </xs:complexType>

  <xs:complexType name="accountCustomMetadata">
    <xs:complexContent>
      <xs:extension base="tns:accountKeyValuePairs">
        <xs:sequence/>
        <xs:attribute name="section" type="xs:string"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="accountKeyValuePairs">
    <xs:sequence>
      <xs:element name="a" type="ns1:keyValuePair" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="contactGroupMember">
    <xs:sequence>
      <xs:element name="cn" type="tns:contactInfo" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="type" type="xs:string" use="required"/>
    <xs:attribute name="value" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="changePasswordRequest">
    <xs:all>
      <xs:element name="account" type="ns1:accountSelector"/>
      <xs:element name="oldPassword" type="xs:string"/>
      <xs:element name="password" type="xs:string"/>
      <xs:element name="virtualHost" type="xs:string" minOccurs="0"/>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="changePasswordResponse">
    <xs:all>
      <xs:element name="authToken" type="xs:string"/>
      <xs:element name="lifetime" type="xs:long"/>
    </xs:all>
  </xs:complexType>

  <xs:complexType name="checkLicenseRequest">
    <xs:sequence/>
    <xs:attribute name="feature" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="checkLicenseResponse">
    <xs:sequence/>
    <xs:attribute name="status" type="tns:checkLicenseStatus" use="required"/>
  </xs:complexType>

  <xs:complexType name="checkRightsRequest">
    <xs:sequence>
      <xs:element name="target" type="tns:checkRightsTargetSpec" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="checkRightsTargetSpec">
    <xs:sequence>
      <xs:element name="right" type="xs:string" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="type" type="ns1:targetType" use="required"/>
    <xs:attribute name="by" type="ns1:targetBy" use="required"/>
    <xs:attribute name="key" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="checkRightsResponse">
    <xs:sequence>
      <xs:element name="target" type="tns:checkRightsTargetInfo" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="checkRightsTargetInfo">
    <xs:sequence>
      <xs:element name="right" type="tns:checkRightsRightInfo" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="type" type="ns1:targetType" use="required"/>
    <xs:attribute name="by" type="ns1:targetBy" use="required"/>
    <xs:attribute name="key" type="xs:string" use="required"/>
    <xs:attribute name="allow" type="xs:boolean" use="required"/>
  </xs:complexType>

  <xs:complexType name="checkRightsRightInfo">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="allow" type="xs:boolean" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="createDistributionListRequest">
    <xs:complexContent>
      <xs:extension base="tns:accountKeyValuePairs">
        <xs:sequence/>
        <xs:attribute name="name" type="xs:string" use="required"/>
        <xs:attribute name="dynamic" type="xs:boolean"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="createDistributionListResponse">
    <xs:sequence>
      <xs:element name="dl" type="tns:dlInfo"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="dlInfo">
    <xs:complexContent>
      <xs:extension base="tns:objectInfo">
        <xs:all/>
        <xs:attribute name="ref" type="xs:string" use="required"/>
        <xs:attribute name="d" type="xs:string"/>
        <xs:attribute name="dynamic" type="xs:boolean"/>
        <xs:attribute name="via" type="xs:string"/>
        <xs:attribute name="isOwner" type="xs:boolean"/>
        <xs:attribute name="isMember" type="xs:boolean"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="objectInfo">
    <xs:sequence>
      <xs:element name="a" type="ns1:keyValuePair" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="name" type="xs:string" use="required"/>
    <xs:attribute name="id" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="createIdentityRequest">
    <xs:sequence>
      <xs:element name="identity" type="tns:identity"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="identity">
    <xs:complexContent>
      <xs:extension base="tns:attrsImpl">
        <xs:sequence/>
        <xs:attribute name="name" type="xs:string"/>
        <xs:attribute name="id" type="xs:string"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="attrsImpl" abstract="true">
    <xs:sequence>
      <xs:element name="a" type="tns:attr" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="createIdentityResponse">
    <xs:sequence>
      <xs:element name="identity" type="tns:identity"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="createSignatureRequest">
    <xs:sequence>
      <xs:element ref="tns:signature"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="signature">
    <xs:sequence>
      <xs:element name="content" type="tns:signatureContent" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="cid" type="xs:string" minOccurs="0"/>
    </xs:sequence>
    <xs:attribute name="id" type="xs:string"/>
    <xs:attribute name="name" type="xs:string"/>
  </xs:complexType>

  <xs:complexType name="signatureContent">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="type" type="xs:string"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>

  <xs:complexType name="createSignatureResponse">
    <xs:sequence>
      <xs:element name="signature" type="tns:nameId"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="nameId">
    <xs:sequence/>
    <xs:attribute name="name" type="xs:string"/>
    <xs:attribute name="id" type="xs:string"/>
  </xs:complexType>

  <xs:complexType name="deleteIdentityRequest">
    <xs:sequence>
      <xs:element name="identity" type="tns:nameId"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="deleteIdentityResponse">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="deleteSignatureRequest">
    <xs:sequence>
      <xs:element name="signature" type="tns:nameId"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="deleteSignatureResponse">
    <xs:sequence/>
  </xs:complexType>

  <xs:complexType name="discoverRightsRequest">
    <xs:sequence>
      <xs:element name="right" type="xs:string" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="discoverRightsResponse">
    <xs:sequence>
      <xs:element name="targets" type="tns:discoverRightsInfo" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="discoverRightsInfo">
    <xs:sequence>
      <xs:element name="target" type="tns:discoverRightsTarget" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="right" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="discoverRightsTarget">
    <xs:sequence>
      <xs:element name="email" type="tns:discoverRightsEmail" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
    <xs:attribute name="type" type="ns1:targetType" use="required"/>
    <xs:attribute name="id" type="xs:string"/>
    <xs:attribute name="name" type="xs:string"/>
    <xs:attribute name="d" type="xs:string"/>
  </xs:complexType>

  <xs:complexType name="discoverRightsEmail">
    <xs:sequence/>
    <xs:attribute name="addr" type="xs:string" use="required"/>
  </xs:complexType>

  <xs:complexType name="distributionListActionRequest">
    <xs:complexContent>
      <xs:extension base="tns:attrsImpl">
        <xs:sequence>
          <xs:element name="dl" type="ns1:distributionListSelector"/>
          <xs:element name="action" type="tns:distributionListAction"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="distributionListAction">
    <xs:complexContent>
      <xs:extension base="tns:accountKeyValuePairs">
        <xs:sequence>
          <xs:element name="dlm" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="newName" type="xs:string" minOccurs="0"/>
          <xs:element name="owner" type="tns:distributionListGranteeSelector" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="right" type="tns:distributionListRightSpec" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="tns:subsReq" minOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="op" type="tns:operation" use="required"/>
      </xs:extension>
    </xs:complexContent>
